{
    "name": "proper-playwright",
    "version": "1.0.0",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "lint": "eslint \"src/**/*.ts\"",
        "lint:fix": "eslint \"src/**/*.ts\" --fix",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{ts,js}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,yml,yaml}": [
            "prettier --write"
        ]
    },
    "keywords": [],
    "author": "Mugilan R",
    "license": "ISC",
    "description": "This is playwright typescript framework",
    "devDependencies": {
        "@eslint/js": "^9.30.1",
        "@playwright/test": "^1.55.1",
        "@types/node": "^24.0.10",
        "allure-playwright": "^3.4.1",
        "dotenv": "^17.2.2",
        "eslint": "^9.30.1",
        "eslint-plugin": "^1.0.1",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "prettier": "^3.6.2",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.35.1"
    }
}
